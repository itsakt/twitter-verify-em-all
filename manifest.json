{
  "name": "Verify em all",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Add verified badges to all twitter accounts.",
  "author": "AKT",
  "homepage_url": "https://github.com/itsakt/twitter-verify-em-all",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "https://*.twitter.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.twitter.com/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}